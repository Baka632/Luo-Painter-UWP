<UserControl
    x:Class="Luo_Painter.Controls.PressurePanel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignWidth="200"
    d:DesignHeight="200"
    >

    <Canvas Width="120" Height="120" >
        <Line x:Name="Line25" X1="30" Y1="120" X2="30" Y2="120" StrokeDashArray="6 4" StrokeThickness="1" Stroke="{ThemeResource BackButtonDisabledForegroundThemeBrush}"/>
        <Line x:Name="Line50" X1="60" Y1="120" X2="60" Y2="120" StrokeDashArray="6 4" StrokeThickness="1" Stroke="{ThemeResource BackButtonDisabledForegroundThemeBrush}"/>
        <Line x:Name="Line75" X1="90" Y1="120" X2="90" Y2="120" StrokeDashArray="6 4" StrokeThickness="1" Stroke="{ThemeResource BackButtonDisabledForegroundThemeBrush}"/>
        <Polyline x:Name="Polyline" StrokeThickness="1" Stroke="White" Fill="{ThemeResource SystemControlBackgroundBaseLowBrush}"/>
    </Canvas>
</UserControl>